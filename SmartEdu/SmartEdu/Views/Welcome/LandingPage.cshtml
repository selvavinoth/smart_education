@model SmartEdu.ViewModel.StudentViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>@ViewBag.Title</title>
    @Html.Partial("_BootStrap")
    @*<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>*@
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
     <script type="text/javascript">
         $(function () { fnGetMenu(); });
         function fnGetMenu() {
             $.ajax({
                 url: '@Url.Content("~/Welcome/BootStrapMenuData")',
                 type: "POST",
                 dataType: "Json",
                 data: {},
                 success: function (obj) {
                     if (obj && obj != null) {
                         $("#menuDiv").html(obj);
                         fnInitMenu();
                        // fnScript();
                     }
                 },
                 error: function (a, b) { }
             });
         }
         function fnLoadFrame(url) { alert(url); $("#icontent").attr("src", url); }
     </script>
</head>
<body>
    <section id="container" class="">
      <!--header start-->
      <header class="header head-bg">
          <div class="sidebar-toggle-box">
              <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
          </div>
          <!--logo start-->
          <a href="index.html" class="logo" >
            <span>
              School Management System            </span></a>
          <!--logo end-->
          <div class="top-nav ">
              <ul class="nav pull-right top-menu">
                  <!-- user login dropdown start-->
                  <li class="dropdown">
                      <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                          <img alt="" src= "../../Content/BootStrap/img/avatar1_small.jpg">
                          <span class="username">
                            <font style='color:#fff; size:24px;'>Mr. Admin</font>                          </span>
                          <b class="caret"></b>
                      </a>
                      <ul class="dropdown-menu extended logout">
                          <div class="log-arrow-up"></div>
                          <li><a href="#" ><div id="test" onclick="vi()"><i class=" icon-suitcase"></i>Profile</div></a></li>
                          <li><a href="settings.html"><i class="icon-cog"></i> Settings</a></li>
                          <li><a href="logout.html"><i class="icon-key"></i> Log Out</a></li>
                      </ul>
                  </li>
                  <!-- user login dropdown end -->
              </ul>
          </div>
      </header>
      <!--header end-->
      <!--sidebar start-->
      <aside id="menuDiv">
     @*  @Html.Raw(ViewBag.MenuData)*@
      </aside>
      <!--sidebar end-->

       <div id="StudentDetail" style="overflow:auto; display:none:">
            @Html.Partial("_tst", "") 
    </div>

      <!--main content start-->
      <section id="main-content">
          <section class="wrapper" id="iFrameSection">
             <iframe id="icontent" src="about:blank" frameborder="0" scrolling="no" height="100%" width="100%">
                               
            </iframe>
              <!-- page end-->
          </section>
      </section>
      <!--main content end-->
      <!--footer start-->
      <footer class="site-footer">
          <div class="text-center">
              2014 &copy;
              <a href="#" class="go-top">
                  <i class="icon-angle-up"></i>
              </a>
          </div>
      </footer>
      <!--footer end-->
  </section>
<script src='@Url.Content("~/Content/BootStrap/js/jquery.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/css3-mediaqueries.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/bootstrap.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/jquery.dcjqaccordion.2.7.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/jquery.scrollTo.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/jquery.nicescroll.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/respond.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Content/BootStrap/js/common-scripts.js")' type="text/javascript"></script>

<script type="text/javascript">
    var tH = $(window).height();
    var tW = $(window).width();
    var pH = 100;
    var fH = 30;
    var aW = 250;
    var bH = tH - (pH + fH);
    var bW = tW - aW;
    function fnInitPageSize() {
        $("#iFrameSection").height(bH + "px");
        $("#iFrameSection").width(bW + "px");
    }
    $(function () { fnInitPageSize(); });
    function ContentHeight() {return bH; }
    function ContentWidth() { return bW; }


    function vi() {
        //        $("#StudentDetail").show();
        alert('boss');
        $("#StudentDetail").attr("display", "block");
    }	


</script>
</body>

</html>
